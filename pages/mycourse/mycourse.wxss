.title-only {
  padding: 20rpx 0;
}

.card-title-wrap {
  display: inline-block;
  width: 80%;
}

.card-btn-wrap {
  display: inline-block;
  width: 20%;
  text-align: center;
}

.btn-view {
  font-size: 24rpx;
  border-radius: 30rpx;
  border: solid 2rpx #999999;
  display: inline-block;
  padding: 10rpx 26rpx;
}

.btn-view-warning{
  border: solid 2rpx #f74444;
  color: #f74444;
}

.btn-view-hover{
  background:#f1f1f1;
}
.btn-view-add {}


.weui-input-inline{
  display: inline-block;
  width: 42%;
  vertical-align: middle;
}

